{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Jwt": {
        "Key": "YourSecretKeyForAuthenticationOfApplication",
        "Issuer": "Imbulii",
        "Audience": "Imbulii",
        "Expires": "7"
    },
    "Cors": {
        "Origins": [
            "https://localhost:7079",
            "https://localhost:5173"
        ],
        "Headers": [
            "Content-Type",
            "Authorization"
        ],
        "Methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
        ]
    },
    "Roles": [ "User", "Admin" ],
    "ConnectionStrings": {
        "DefaultConnection": "Server=localhost;Port=5432;Database=GotExplorer;Username=example;Password=example"
    },
    "Smtp": {
        "Host": "",
        "Port": 587,
        "EnableSSL": true,
        "UseDefaultCredentials": false,
        "Username": "",
        "Password": "",
        "From": ""
    },
    // Sizes are in bytes
    "UploadFileLimits": {
        "MaxImageSize": 2097152,
        "MaxModel3dSize": 10485760
    }
}
